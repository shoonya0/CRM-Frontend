{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://crm-backend-zeta-eight.vercel.app/"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }